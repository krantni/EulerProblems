{
    "extends": [
        "tslint-config-airbnb",
        "tslint-config-prettier",
        "tslint-plugin-prettier"
    ],
    "rules": {
        "no-any":true,
        "import-name": false,
        "strict-boolean-expressions": true,
        "no-increment-decrement": false,
        "semicolon": false,
        "variable-name":[
            true,
            "check-format",
            "allow-pascal-case"
        ]
    },
    "jsRules": {
        "no-eval":true
    }
  }